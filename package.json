{
  "name": "tuuduu",
  "version": "1.0.0",
  "description": "Simple todo manager built with vanilla js and express.js",
  "type": "module",
  "scripts": {
    "start": "node index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "stylelint": "stylelint \"**/*.css\"",
    "w3c": "html-validator --file ./source/public/index.html  --verbose",
    "eslint": "eslint \"**/*.js\"",
    "all": "npm run stylelint && npm run w3c  && npm run eslint  && node -e \"console.log('npm run all completed')\""
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=16.0.0"
  },
  "author": "Cornel Widmer",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^8.14.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "html-validator-cli": "^7.0.1",
    "minimist": ">=1.2.6",
    "prettier": "2.6.2",
    "stylelint": "^14.7.1",
    "stylelint-config-standard": "^25.0.0"
  },
  "dependencies": {
    "body-parser": "^1.20.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "nedb-promises": "^6.0.3",
    "party-js": "^2.1.3",
    "path": "^0.12.7"
  }
}
